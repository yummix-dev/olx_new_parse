# Proxy настройки
# Список IP адресов прокси через запятую
PROXIES_IP=127.0.0.1,127.0.0.2,127.0.0.3

# Порт прокси
PROXY_PORT=1050

# Логин для прокси (оставьте пустым если не требуется)
PROXY_LOGIN=your_proxy_login

# Пароль для прокси (оставьте пустым если не требуется)
PROXY_PASSWORD=your_proxy_password

# Parser настройки
# Таймаут запросов в секундах (по умолчанию 10)
REQUEST_TIMEOUT=10

# Максимальное количество попыток retry (по умолчанию 3)
MAX_RETRIES=3

# URL полигонального сервиса
POLYGON_SERVICE_URL=http://194.87.56.245/search

# Разрешенные домены для парсинга (через запятую)
ALLOWED_DOMAINS=olx.uz,www.olx.uz

# База данных MySQL
DB_USER=root
DB_PASSWORD=your_password
DB_HOST=localhost
DB_PORT=3306
DB_NAME=xestate
DB_ECHO=False

# RabbitMQ
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
